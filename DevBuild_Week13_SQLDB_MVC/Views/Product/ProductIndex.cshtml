@{
    ViewData["Title"] = "Home Page";
    List<Product> Products = (List<Product>)ViewData["Products"];
}

<div>
    <h1 class="display-4">Menu:</h1>

    <ul>
        @foreach (Product p in Products)
        {
            <li><a href="/Product/Category?cat=@p.Category">@p.Category</a></li>
        }
    </ul>

</div>
